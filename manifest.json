{
  "name": "Japanese Vocab",
  "version": "1.2.1",
  "description": "Presents a small series of Japanese Vocabulary flashcards upon opening or closing a tab",
  "manifest_version": 2,

  "browser_action": {
      "default_icon": "icon-active.png",
      "default_title": "Japanese Vocab Test: Active"
  },

  "background": {
      "page": "background.html"
  },

  "options_page": "options.html",

  "permissions": [
    "tabs", "http://*/*"
  ]
}
